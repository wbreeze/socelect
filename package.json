{
  "name": "socelect",
  "version": "1.0.0",
  "description": "JavaScript package for socelect",
  "main": "src/index.js",
  "repository": "git@github.com:wbreeze/socelect.git",
  "author": "Douglas Lovell <doug@wbreeze.com>",
  "license": "GPL-3.0",
  "scripts": {
    "clean": "rm app/assets/javascript/parceled/*",
    "build-prod": "parcel build src/index.js --out-dir app/assets/javascripts/parceled --no-content-hash",
    "serve": "parcel src/index.html --out-dir app/assets/javascripts/parceled",
    "watch": "parcel watch src/index.js --out-dir app/assets/javascripts/parceled"
  },
  "dependencies": {
    "poui": "^1.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-simple-timefield": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "parcel-bundler": "^1.12.3"
  }
}
