{
  "name": "socelect",
  "version": "1.0.0",
  "description": "JavaScript package for socelect",
  "browserslist": "> 0.2% or last 1 version and not dead",
  "repository": "git@github.com:wbreeze/socelect.git",
  "author": "Douglas Lovell <doug@wbreeze.com>",
  "license": "GPL-3.0",
  "scripts": {
    "clean": "rm -rf app/assets/javascripts/parceled",
    "build": "yarn run clean && NODE_ENV='production' parcel build src/index.js --out-dir app/assets/javascripts/parceled --no-source-maps --no-content-hash"
  },
  "dependencies": {
    "core-js": "3",
    "poui": "^1.3.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-simple-timefield": "^3.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0"
  }
}
